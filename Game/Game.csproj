        <Nullable>enable</Nullable>
        <UseWindowsForms>true</UseWindowsForms>
        <ImplicitUsings>enable</ImplicitUsings>
        <RootNamespace>WinFormsApp1</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
      <Compile Update="Forms\Welcome.cs">
        <SubType>Form</SubType>
      </Compile>
      <Compile Update="Forms\UsernameInput.cs">
        <SubType>Form</SubType>
      </Compile>
      <Compile Update="Forms\Game.cs">
        <SubType>Form</SubType>
      </Compile>
      <Compile Update="Forms\StatsViewer.cs">
        <SubType>Form</SubType>
      </Compile>
    </ItemGroup>

</Project>